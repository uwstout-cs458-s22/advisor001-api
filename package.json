{
  "name": "advisor-001-api",
  "version": "1.0.0",
  "description": "Program Course Advisor API",
  "main": "server.js",
  "repository": "https://github.com/uwstout-cs458-s22/advisor001-api",
  "author": "UWStout CS458-001 Spring 2022",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "npx jest && npx eslint ./models/* ./routes/* ./services/* ./*.js",
    "test:debug": "npx jest --runInBand"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "dotenv": "^10.0.0",
    "eslint-plugin-jest": "^25.3.3",
    "express": "^4.16.3",
    "loglevel": "^1.8.0",
    "loglevel-plugin-prefix": "^0.8.4",
    "path": "^0.12.7",
    "pg": "^8.7.1",
    "pg-connection-string": "^2.5.0",
    "stytch": "^3.6.1"
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@jest-mock/express": "^1.4.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "models/*",
      "routes/*",
      "services/*",
      "*.js"
    ],
    "coveragePathIgnorePatterns": [
      "services/stytchwrapper.js",
      "services/database.js",
      "services/environment.js",
      "./server.js"
    ],
    "setupFilesAfterEnv": [
      "./jest/tests_common"
    ],
    "testPathIgnorePatterns": [
      "jest/*"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 90
      }
    }
  }
}
